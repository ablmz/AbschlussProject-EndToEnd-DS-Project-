  {
   "cell_type": "code",
   "execution_count": 416,
   "metadata": {},
   "outputs": [],
   "source": [
    "bewertunglist=['Gesamtzufriedenheit','Qualität der Beratung','Mediz. Behandlung','Verwaltung und Abläufe','Ausstattung und Gestaltung']\n",
    "sternelist=['Gesamt','Qualität','Behandlung','Verwaltung','Ausstattung']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 417,
   "metadata": {},
   "outputs": [],
   "source": [
    "for w in range(len(bewertunglist)):\n",
    "    df[bewertunglist[w]]=df[bewertunglist[w]].str.replace(r\"sehr zufrieden \",\"sehr zufrieden\")\n",
    "    df[bewertunglist[w]]=df[bewertunglist[w]].str.replace(r\"weniger zufrieden \",\"weniger zufrieden\")\n",
    "    df[bewertunglist[w]]=df[bewertunglist[w]].str.replace(r\"zufrieden \",\"zufrieden\")\n",
    "    df[bewertunglist[w]]=df[bewertunglist[w]].str.replace(r\"unzufrieden \",\"zufrieden\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 418,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array(['sehr zufrieden', 'unzufrieden', 'zufrieden', 'weniger zufrieden'],\n",
