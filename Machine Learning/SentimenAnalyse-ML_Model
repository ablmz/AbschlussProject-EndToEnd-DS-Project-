    "def sentiment(klinikname):\n",
    "    Erfahrunsberichten=\"\"\n",
    "    for i in range(df.loc[df[\"Name der Klinik\"] == klinikname, 'Name der Klinik'].count()):\n",
    "        Erfahrunsberichten+=df['Erfahrungsbericht'][i]\n",
    "    return Erfahrunsberichten"
